{% extends '::base.html.twig' %}
{% block body %}

<div id="formulario">
  {{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}
    {{ form_row(form.guardar) }}
    {{ form_row(form.borrar) }}
  {{ form_end(form) }}

</div>

{% endblock %}
